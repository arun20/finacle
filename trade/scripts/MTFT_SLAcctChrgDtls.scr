			#EXITSCRIPT
		#}
		ENDIF

		### INSERT FROM TBAADM.DTD TO CUSTOM.C_DTD_P_EXT ###
		sv_a = func_selectFromCustTableB("count","count(*)","TBAADM.DTD","TRIM(TRAN_ID)|TO_CHAR(TRAN_DATE,'DD-MM-YYYY')", TRIM(CUST.DATA.chrgTranId) + "|" + CUST.DATA.chrgStartDate)
		PRINT(sv_a)
