		#{
			### GET CHARGE RELATED DETAILS ###
			CUST.DATA.srvFieldNames  = "pcChargeDtl.entityChargeLL.<rec_"+CUST.DATA.counter+">.chrgEventId.eventId|pcChargeDtl.entityChargeLL.<rec_"+CUST.DATA.counter+">.chrgEventId.eventType|pcChargeDtl.entityChargeLL.<rec_"+CUST.DATA.counter+">.tfctSrlNum|pcChargeDtl.entityChargeLL.<rec_"+CUST.DATA.counter+">.crDrInd"
